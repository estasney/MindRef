#: import TextTyper widgets.typeahead.typer

<TypeAhead>:
    orientation: 'vertical'
    typer: typer
    size_hint_y: None
    height: typer.minimum_height
    TextTyper:
        id: typer
        multiline: False
        on_text: self.handle_text(self, self.text)
        font_size: app.base_font_size
        mipmap: True
